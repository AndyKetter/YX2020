        "analyz:build": "cross-env NODE_ENV=production use_analyz=true npm run build",
        "analyz:pre": "cross-env NODE_ENV=pre use_analyz=true npm run pre",
        "analyz:dev": "cross-env NODE_ENV=dev use_analyz=true npm run dev",
